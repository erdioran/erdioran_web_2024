<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Kariyer UI Test Suite" parallel="tests" data-provider-thread-count="1">
    <listeners>
        <listener class-name="com.kariyer.listener.ExecutionListener"/>
        <listener class-name="com.kariyer.listener.TestListener"/>
        <listener class-name="com.kariyer.listener.AnnotationTransformer"/>
    </listeners>
    <test name="Run Forgot Password Tests">
        <classes>
            <class name="testCases.ForgotPasswordTests"/>
        </classes>
    </test>
</suite>