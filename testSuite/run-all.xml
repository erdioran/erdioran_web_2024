<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Kariyer Suite" verbose="1" thread-count="1">
  <listeners>
    <listener class-name="com.kariyer.listener.ExecutionListener"/>
    <listener class-name="com.kariyer.listener.SuiteListener"/>
    <listener class-name="com.kariyer.listener.TestListener"/>
    <listener class-name="com.kariyer.listener.AnnotationTransformer"/>
  </listeners>
  <test name="Login Scenarios" enabled="true">
    <classes>
      <class  name="testCases.LoginTests">
      </class>
    </classes>
  </test>
  <test name="Forgot Password Scenarios" enabled="true">
    <classes>
      <class name="testCases.ForgotPasswordTests">
      </class>
    </classes>
  </test>
</suite>